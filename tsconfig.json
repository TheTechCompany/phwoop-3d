  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "noResolve": false,
    "noImplicitAny": false,
    "allowJs": true,
    "maxNodeModuleJsDepth": 1,
    "esModuleInterop": true,
    "sourceMap": true,
    "preserveConstEnums": true,
    "lib": [
      "dom",
      "es6"
    ],
    "rootDir": "src"
  }
}
